#quiero_firmar
  margin-bottom 60px
  padding-top 20px
  max-width 960px

  h1
    text-align center

.quiero-firmar-map
  position relative
  min-height 500px
  width 100%
  margin-bottom -50px
  overflow hidden
  z-index 0

  &:after
    content ''
    display block
    padding-top 41%

  #quiero
    position absolute
    top 96px
    right 56%
    padding 40px 20px
    text-align center
    width 360px
    z-index 10
    background #fff

    h2
      margin-top 0

    p
      margin-bottom 20px

  iframe
    position absolute
    left 0
    top -50px
    width 100%
    height 100%
    z-index 0
