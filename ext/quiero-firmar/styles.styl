#quiero_firmar
  margin-bottom 60px
  padding-top 20px
  max-width 960px

  h1
    text-align center

.quiero-firmar-map
  position relative
  width 100%
  overflow hidden
  z-index 0
  transform unquote('translate3d(0,0,0)')

  .quiero-firmar
    position absolute
    top 96px
    right 56%
    padding 40px 20px
    text-align center
    width 360px
    z-index 10
    background #fff

    h2
      margin-top 0

    p
      margin-bottom 20px

  .map-container
    position relative
    min-height 500px
    z-index 1

    &:after
      content ''
      display block
      padding-top 41%

    .overlay
      position absolute
      left 0
      top 0
      width 100%
      height 100%
      cursor pointer

    iframe
      position absolute
      left 0
      top -50px
      width 100%
      height 100%
      height unquote('calc(100% + 50px)')
      overflow hidden
